{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}